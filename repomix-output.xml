                # Processar CEP
                cep_limpo = cep_raw.replace('CEP: ', '').strip()
                
                return {
                    'endereco': endereco_limpo,
                    'bairro': bairro_limpo,
                    'cidade': cidade_limpa,
                    'uf': uf_limpa,
