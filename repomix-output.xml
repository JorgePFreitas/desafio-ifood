def extract_minimum_order(soup):
    """Extrai pedido mínimo."""
    try:
        min_order_div = soup.find('div', class_='merchant-info__minimum-order')
        if min_order_div:
            text = min_order_div.get_text()
            match = re.search(r'R\$\s*([\d,]+\.?\d*)', text)
            if match:
                value = match.group(1).replace(',', '.')
                return float(value)
    except:
        pass
    return 0.0


